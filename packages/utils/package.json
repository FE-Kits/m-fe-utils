{
  "name": "@m-fe/utils",
  "version": "0.3.2",
  "description": "@m-fe/utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/FE-Kits/m-fe-utils"
  },
  "author": "wx-chevalier@github",
  "license": "MIT",
  "main": "dist/cjs",
  "types": "dist/types",
  "files": [
    "dist/"
  ],
  "keywords": [
    "webpack",
    "react"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run copy",
    "build:cjs": "tsc --project ./tsconfig.cjs.json && npm run copy",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 './src/**/*.(less|svg)' dist/cjs/",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,ts,tsx -f friendly --max-warnings 10",
    "lint:ts": "tslint -p . -t stylish --fix",
    "lint:tsc": "tsc -p tsconfig.json --incremental false --noEmit",
    "pub": "npm run clean && npm run build && npm publish",
    "start": "npm run dev",
    "test": "jest --config ../../scripts/jest/jest.config.js"
  },
  "dependencies": {
    "dayjs": "^1.8.26",
    "fast-deep-equal": "^2.0.1",
    "history": "^4.10.1",
    "lodash.chunk": "^4.2.0",
    "lodash.upperfirst": "^4.3.1",
    "urijs": "^1.19.2",
    "uuid": "^3.3.3",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@m-fe/app-config": "^0.4.2",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/lodash-es": "^4.17.3",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/urijs": "^1.19.5",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "autoprefixer": "9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.3",
    "copyfiles": "^2.1.0",
    "css-loader": "2.1.1",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.3",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "json-server": "0.14.2",
    "lerna": "^3.14.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallelshell": "^3.0.2",
    "parcel-bundler": "^1.12.3",
    "postcss-loader": "3.0.0",
    "prettier": "^1.17.1",
    "react-hot-loader": "^4.8.5",
    "resolve-url-loader": "3.1.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "^1.1.2",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dashboard": "3.0.7",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": "@m-fe/prettier-config/semi"
}
